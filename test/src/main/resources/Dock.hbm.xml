<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="entity.Dock" table="dock" schema="HibernateBasic">
        <id name="id">
            <column name="Id" sql-type="int"/>
        </id>
        <property name="name">
            <column name="name" sql-type="varchar(100)" length="100"/>
        </property>
        <property name="numEmptyDockPoint">
            <column name="numEmptyDockPoint" sql-type="int"/>
        </property>
        <property name="numAvailableBike">
            <column name="numAvailableBike" sql-type="int"/>
        </property>
        <property name="area">
            <column name="area" sql-type="int"/>
        </property>
        <property name="address">
            <column name="address" sql-type="varchar(200)" length="200"/>
        </property>
    </class>
</hibernate-mapping>