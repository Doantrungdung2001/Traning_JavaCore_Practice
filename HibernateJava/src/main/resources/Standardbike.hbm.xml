<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="entity.Standardbike" table="standardbike" schema="ecobikerental">
        <property name="id">
            <column name="Id" sql-type="char(16)" length="16"/>
        </property>
        <many-to-one name="bikeById" class="entity.Bike">
            <column name="Id"/>
        </many-to-one>
    </class>
</hibernate-mapping>