<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="entity.Bike" table="bike" schema="ecobikerental">
        <id name="id">
            <column name="Id" sql-type="char(16)" length="16"/>
        </id>
        <property name="idDock">
            <column name="Id_Dock" sql-type="char(16)" length="16"/>
        </property>
        <property name="licensePlate">
            <column name="licensePlate" sql-type="varchar(20)" length="20" not-null="true"/>
        </property>
        <property name="barcode">
            <column name="barcode" sql-type="varchar(6)" length="6"/>
        </property>
        <property name="status">
            <column name="status" sql-type="int" not-null="true"/>
        </property>
        <property name="type">
            <column name="type" sql-type="int" not-null="true"/>
        </property>
        <property name="value">
            <column name="value" sql-type="int" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>