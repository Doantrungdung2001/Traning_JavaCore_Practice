<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="entity.Standardbike" table="standardbike" schema="ecobikerental">
        <id name="id" type="java.lang.String">
            <column name="Id" sql-type="char(16)" length="16"/>
        </id>
        <one-to-one name="bikeById" class="entity.Bike"/>
    </class>
</hibernate-mapping>